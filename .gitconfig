[user]
    name = Alexis Daboville
    email = alexis.daboville@gmail.com
[color]
    diff = auto
    status = auto
    branch = auto
[alias]
    ci = commit
    co = checkout
    di = diff
    br = branch
    me = merge
    st = status -s
    pop = stash pop
    l = log --graph --all --date=relative\
        --pretty=format:'%C(cyan)%h%Creset %s %C(green)[%an/%cr]%Creset%C(red)%d%Creset'
    unstage = reset HEAD
    last = log -1 HEAD
    update-all-submodules = submodule foreach git pull origin master
    forget=update-index --assume-unchanged
[core]
    editor = vim
[merge]
    tool = vimdiff
[pull]
    default = current
[push]
    default = current
